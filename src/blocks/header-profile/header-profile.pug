                span Deposit

            +b("a")(href="#", class="button")
                i(class="icon-upload")
                span Withdraw

        +e("nav")(class="menu")
            +e("a")(href="#", class="link") Funding Wallet
            +e("a")(href="#", class="link") Spot Wallet
            +e("a")(href="#", class="link") Open Orders
            +e("a")(href="#", class="link") Order History

        +e("a")(href="#", class="link _logout")
            i(class="icon-upload")
            span Log out
